<?xml version = "1.0"?>

<simulation>
	<basicinfo>
		<simulationName>SugarScape</simulationName>
		<simulationTitle>Our Amazing Game</simulationTitle>
		<simulationAuthor>Eric Fu, Frank Yin, Jing Yang</simulationAuthor>
	</basicinfo>
	<globalsettings>
		<propertystring>ZeroPatch,OnePatch,TwoPatch,ThreePatch,FourPatch,Agent</propertystring>
		<colors>#FFFFFF,#FFD700,#FFA500,#FF8C00,#FF7F50,#FF0000</colors>
		<cellshape>square</cellshape>
		<useprobability>1</useprobability>
	</globalsettings>
	<gridparameter>
		<sugarinterval>5</sugarinterval>
		<sugarmetabolism>2</sugarmetabolism>
	</gridparameter>
	<cellparameter>
		<initialenergy>10</initialenergy>
		<vision>0.2</vision>
	</cellparameter>
	<calculatorparameter>
		<reproducerate>10</reproducerate>
	</calculatorparameter>
	<gridconfig>
		<gridheight>20</gridheight>
		<gridwidth>20</gridwidth>
	</gridconfig>
	<percentages>
		<zeropatchpercent>0</zeropatchpercent>
		<onepatchpercent>0.1</onepatchpercent>
		<twopatchpercent>0.2</twopatchpercent>
		<threepatchpercent>0.2</threepatchpercent>
		<fourpatchpercent>0.4</fourpatchpercent>
		<agentpercent>0.1</agentpercent>
	</percentages>
	<cellstates>
		<cell x = "0" y = "0"><state>0</state></cell>
		<cell x = "1" y = "0"><state>0</state></cell>
		<cell x = "2" y = "0"><state>0</state></cell>
		<cell x = "0" y = "1"><state>0</state></cell>
		<cell x = "1" y = "1"><state>0</state></cell>
		<cell x = "2" y = "1"><state>0</state></cell>
		<cell x = "0" y = "2"><state>0</state></cell>
		<cell x = "1" y = "2"><state>0</state></cell>
		<cell x = "2" y = "2"><state>0</state></cell>
	</cellstates>
</simulation>