<?xml version = "1.0"?>

<simulation>
	<basicinfo>
		<simulationName>Fire</simulationName>
		<simulationTitle>Our Amazing Game</simulationTitle>
		<simulationAuthor>Eric Fu, Frank Yin, Jing Yang</simulationAuthor>
	</basicinfo>
	<globalsettings>
		<propertystring>Tree,Burning,Empty</propertystring>
		<colors>#00FF00,#FF0000,#FFFF00</colors>
		<cellshape>triangle</cellshape>
		<useprobability>1</useprobability>
	</globalsettings>
	<gridparameter></gridparameter>
	<cellparameter></cellparameter>
	<calculatorparameter>
		<probcatch>0.4</probcatch>
	</calculatorparameter>
